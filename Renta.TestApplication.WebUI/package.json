{
  "name": "WeAre.Athenaeum.TemplateApp.WebUI",
  "private": true,
  "dependencies": {
    "async-mutex": "^0.1.3",
    "bootstrap": "^4.3.1",
    "jquery": "^3.2.1",
    "popper.js": "^1.14.7",
    "query-string": "^6.8.2",
    "rc-slider": "^8.6.13",
    "react": "^16.8.2",
    "react-datepicker": "^2.5.0",
    "react-dependent-script": "^1.1.2",
    "react-dom": "^16.8.2",
    "react-google-autocomplete": "^1.1.2",
    "react-qr-reader": "^2.2.1",
    "react-signature-canvas": "^1.0.1"
  },
  "devDependencies": {
    "@types/bootstrap": "^4.3.0",
    "@types/googlemaps": "^3.39.5",
    "@types/jquery": "^3.3.29",
    "@types/node": "11.9.4",
    "@types/rc-slider": "^8.6.4",
    "@types/react": "16.8.3",
    "@types/react-datepicker": "^2.3.0",
    "@types/react-dom": "16.8.2",
    "@types/react-qr-reader": "^2.1.2",
    "@types/react-table": "^6.8.3",
    "@types/ts-nameof": "^4.2.1",
    "babel-plugin-ts-nameof": "^4.2.1",
    "customize-cra": "^0.8.0",
    "node-sass": "^4.11.0",
    "nodemon": "^2.0.7",
    "react-app-rewired": "^2.1.3",
    "react-scripts": "^3.4.3",
    "ts-node": "^9.1.1",
    "typescript": "3.8",
    "typescript-collections": "^1.3.2"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "prebuild": "mkdir -p src/@weare && npm run copy:toolkit && npm run copy:common && npm run copy:components",
    "build": "react-app-rewired build",
    "copy:components": "rsync -av '../WeAre.Athenaeum.React.Components/src/.' './src/@weare/athenaeum-react-components/' --delete",
    "copy:components:back": "rsync -av './src/@weare/athenaeum-react-components/.' '../WeAre.Athenaeum.React.Components/src/'",
    "watch:components": "nodemon --ext '.' --watch '../WeAre.Athenaeum.React.Components/src/*'  --exec 'npm run copy:components'",
    "watch:components:back": "sleep 1 && nodemon --ext '.' --watch './src/@weare/athenaeum-react-components/*' --exec 'npm run copy:components:back'",
    "copy:common": "rsync -av '../WeAre.Athenaeum.React.Common/src/.' './src/@weare/athenaeum-react-common/' --delete",
    "copy:common:back": "rsync -av './src/@weare/athenaeum-react-common/.' '../WeAre.Athenaeum.React.Common/src/'",
    "watch:common": "nodemon --ext '.' --watch '../WeAre.Athenaeum.React.Common/src/*'  --exec 'npm run copy:common'",
    "watch:common:back": "sleep 1 && nodemon --ext '.' --watch './src/@weare/athenaeum-react-common/*' --exec 'npm run copy:common:back'",
    "copy:toolkit": "rsync -av '../WeAre.Athenaeum.React.Toolkit/src/.' './src/@weare/athenaeum-toolkit/' --delete --exclude 'tests'",
    "copy:toolkit:back": "rsync -av './src/@weare/athenaeum-toolkit/.' '../WeAre.Athenaeum.React.Toolkit/src/'",
    "watch:toolkit": "nodemon --ext '.' --watch '../WeAre.Athenaeum.React.Toolkit/src/*'  --exec 'npm run copy:toolkit'",
    "watch:toolkit:back": "sleep 1 && nodemon --ext '.' --watch './src/@weare/athenaeum-toolkit/*'  --exec 'npm run copy:toolkit:back'"
  },
  "browserslist": []
}
