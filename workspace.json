{
  "version": 2,
  "projects": {
    "WeAre.TestApplication.WebUI": {
      "root": "WeAre.TestApplication.WebUI",
      "sourceRoot": "WeAre.TestApplication.WebUI/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "WeAre.TestApplication.WebUI/dist",
            "index": "WeAre.TestApplication.WebUI/src/index.html",
            "main": "WeAre.TestApplication.WebUI/src/main.tsx",
            "polyfills": "WeAre.TestApplication.WebUI/src/polyfills.ts",
            "tsConfig": "WeAre.TestApplication.WebUI/tsconfig.app.json",
            "assets": [
              "WeAre.TestApplication.WebUI/src/favicon.ico",
              "WeAre.TestApplication.WebUI/src/manifest.json",
              "WeAre.TestApplication.WebUI/src/images/logo.svg",
              "WeAre.TestApplication.WebUI/src/images/logox192.png",
              "WeAre.TestApplication.WebUI/src/assets"
            ],
            "styles": [],
            "scripts": [],
            "webpackConfig": "./extra-webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "WeAre.TestApplication.WebUI/src/environments/environment.ts",
                  "with": "WeAre.TestApplication.WebUI/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "WeAre.TestApplication.WebUI:build",
            "hmr": true
          },
          "configurations": {
            "production": {
              "buildTarget": "WeAre.TestApplication.WebUI:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "WeAre.TestApplication.WebUI/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/WeAre.TestApplication.WebUI"],
          "options": {
            "jestConfig": "WeAre.TestApplication.WebUI/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "WeAre.ReApptor.React.Components.Localization": {
