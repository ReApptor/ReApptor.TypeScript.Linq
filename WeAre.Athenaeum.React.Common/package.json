{
  "name": "@weare/athenaeum-react-common",
  "version": "1.0.0",
  "private": false,
  "keywords": [
    "weare",
    "athenaeum",
    "components",
    "react",
    "typescript",
    "npm"
  ],
  "type": "commonjs",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/types/src/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "TYPES_ONLY=true rollup -c=../rollup.config.js && rollup -c=../rollup.config.js",
    "test": "jest -c jest.config.js  --passWithNoTests",
    "coverage": "jest -c jest.config.js --coverage",
    "version:latest": "npm version $(npm view @weare/athenaeum-react-common version)",
    "version:patch": "npm version patch",
    "build:documentation": "npm run build:documentation:extract && npm run build:documentation:copy",
    "build:documentation:extract": "mkdir -p etc && api-extractor run --local --typescript-compiler-folder $(npm root)/typescript",
    "build:documentation:copy": "cp -a ./temp/. ../Documentation/docs-input/"
  },
  "browserslist": [],
  "dependencies": {
    "async-mutex": "^0.1.3",
    "jquery": "^3.2.1",
    "typescript-collections": "^1.3.2"
  },
  "devDependencies": {
    "@microsoft/api-documenter": "^7.13.30",
    "@microsoft/api-extractor": "^7.12.1",
    "@types/jest": "^26.0.14",
    "@types/jquery": "^3.3.29",
    "@types/node": "11.9.4",
    "@types/react": "16.8.3",
    "@types/ts-nameof": "^4.2.1",
    "jest": "^26.4.2",
    "react": "^16.8.2",
    "rollup": "^2.49.0",
    "ts-jest": "^26.4.0",
    "ts-nameof": "^5.0.0",
    "typescript": "3.8",
    "typescript-transform-paths": "^1.1.15"
  },
  "peerDependencies": {
    "@weare/athenaeum-toolkit": "*"
  }
}