{
  "name": "renta-components",
  "version": "1.0.0",
  "main": "index.js",
  "author": "",
  "license": "",
  "private": true,
  "scripts": {
    "install:all": "npm i && npm i --prefix ./WeAre.Athenaeum.React.Components && npm i --prefix ./WeAre.Athenaeum.React.Common && npm i --prefix ./WeAre.Athenaeum.React.Toolkit && npm i --prefix ./Renta.TestApplication.WebUI",
    "start:application:test": "SKIP_PREFLIGHT_CHECK=true DIR=Renta.TestApplication.WebUI react-app-rewired start",
    "build:application:test": "SKIP_PREFLIGHT_CHECK=true DIR=Renta.TestApplication.WebUI react-app-rewired build",
    "build": "npm run build:toolkit && npm run build:common && npm run build:components",
    "build:components": "npm run build --prefix ./WeAre.Athenaeum.React.Components",
    "build:common": "npm run build --prefix ./WeAre.Athenaeum.React.Common",
    "build:toolkit": "npm run build --prefix ./WeAre.Athenaeum.React.Toolkit",
    "test": "npm run test:toolkit && npm run test:common && npm run test:components",
    "test:components": "npm run test --prefix ./WeAre.Athenaeum.React.Components",
    "test:common": "npm run test --prefix ./WeAre.Athenaeum.React.Common",
    "test:toolkit": "npm run test --prefix ./WeAre.Athenaeum.React.Toolkit",
    "build:documentation": "npm run build:markdown && npm run build:markdown:finalize",
    "build:markdown": "api-documenter markdown -i ./Documentation/docs-input/ -o ./Documentation/docs-output/",
    "build:markdown:finalize": "cp ./Documentation/index.html ./Documentation/docs-output/index.html && cp ./Documentation/docs-output/index.md ./Documentation/docs-output/README.md",
    "start:documentation": "npm run build:documentation && npx http-server -o ./Documentation/docs-output"
  },
  "dependencies": {
    "react-signature-canvas": "^1.0.1",
    "async-mutex": "^0.1.3",
    "bootstrap": "^4.3.1",
    "date-fns": "^2.17.0",
    "popper.js": "^1.14.7"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-external-helpers": "^7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-function-bind": "^7.12.13",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@microsoft/api-documenter": "^7.13.30",
    "@microsoft/api-extractor": "^7.12.1",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-image": "^2.0.6",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@types/bootstrap": "^4.3.1",
    "@types/googlemaps": "^3.43.3",
    "@types/jest": "^26.0.14",
    "@types/jquery": "^3.2.1",
    "@types/node": "11.9.4",
    "@types/rc-slider": "^8.6.4",
    "@types/react": "16.8.3",
    "@types/react-datepicker": "^2.3.0",
    "@types/react-dom": "16.8.2",
    "@types/react-qr-reader": "^2.1.2",
    "@types/react-signature-canvas": "^1.0.1",
    "@types/react-table": "^6.8.3",
    "@types/ts-nameof": "^4.2.1",
    "babel-jest": "^24.7.1",
    "babel-plugin-ts-nameof": "^4.2.1",
    "customize-cra": "^0.8.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "getopts": "^2.3.0",
    "identity-obj-proxy": "^3.0.0",
