    "query-string": "^6.14.0",
    "rc-slider": "^8.6.13",
    "react": "^16.8.2",
    "react-app-rewired": "^2.1.3",
    "react-datepicker": "^2.5.0",
    "react-dependent-script": "^1.1.2",
    "react-dev-utils": "^11.0.4",
    "react-dom": "^16.8.2",
    "react-google-autocomplete": "^1.1.2",
    "react-qr-reader": "^2.2.1",
    "react-scripts": "^3.4.3",
    "rollup": "^2.49.0",
    "rollup-plugin-commonjs": "^10.1.0",
